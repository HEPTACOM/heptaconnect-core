{
    "name": "heptacom/heptaconnect-repository-base",
    "description": "HEPTAconnect package",
    "type": "library",
    "keywords": [
        "heptaconnect-dataset",
        "heptaconnect-adapter"
    ],
    "license": [
        "AGPL-3.0-or-later",
        "proprietary"
    ],
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "HEPTACOM GmbH"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "Heptacom\\HeptaConnect\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.1 <10",
        "phpstan/phpstan": ">=0.12",
        "phpstan/extension-installer": ">=1",
        "phpstan/phpstan-phpunit": ">=0.12",
        "phpstan/phpstan-deprecation-rules": ">=0.12",
        "phpstan/phpstan-strict-rules": ">=0.12",
        "psalm/plugin-phpunit": ">=0.10",
        "vimeo/psalm": ">=3",
        "infection/infection": ">=0.15",
        "friendsofphp/php-cs-fixer": ">=2",
        "maglnet/composer-require-checker": ">=2",
        "icanhazstring/composer-unused": ">=0.7"
    },
    "conflict": {
        "nikic/php-parser": "4.7.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Heptacom\\HeptaConnect\\Test\\": "test/"
        }
    }
}
