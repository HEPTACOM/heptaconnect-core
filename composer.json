{
    "name": "heptacom/heptaconnect-core",
    "type": "library",
    "description": "HEPTAconnect core processes",
    "keywords": [
        "heptaconnect",
        "messaging"
    ],
    "version": "0.7.0",
    "license": [
        "AGPL-3.0-or-later",
        "proprietary"
    ],
    "authors": [
        {
            "name": "HEPTACOM GmbH"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-hash": "*",
        "ext-json": "*",
        "ext-zlib": "*",
        "bentools/iterable-functions": "^1.4",
        "composer/composer": ">=1",
        "dragonmantank/cron-expression": ">=3",
        "heptacom/heptaconnect-dataset-base": "self.version",
        "heptacom/heptaconnect-portal-base": "self.version",
        "heptacom/heptaconnect-storage-base": "self.version",
        "league/flysystem": "^1",
        "php-http/discovery": "^1.0",
        "psr/cache": "^1.0",
        "psr/container": "^1.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/log": "^1.1",
        "ramsey/uuid": "^3.5 || ^4",
        "symfony/config": "^4.4 || ^5.0",
        "symfony/dependency-injection": "^4.4 || ^5.0",
        "symfony/lock": "^4.0 || ^5.0",
        "symfony/messenger": "^4.0 || ^5.0",
        "symfony/polyfill-php80": ">=1.15",
        "symfony/yaml": "^4.4 || ^5.0"
    },
    "require-dev": {
        "dq5studios/psalm-junit": ">=2",
        "ergebnis/composer-normalize": ">=2.7",
        "friendsofphp/php-cs-fixer": ">=2.17",
        "icanhazstring/composer-unused": ">=0.7",
        "infection/infection": ">=0.21",
        "maglnet/composer-require-checker": ">=2",
        "mavimo/phpstan-junit": ">=0.3",
        "nyholm/psr7": ">=1",
        "phpmd/phpmd" : "@stable",
        "php-http/curl-client": ">=1",
        "phpstan/extension-installer": ">=1",
        "phpstan/phpstan": ">=0.12",
        "phpstan/phpstan-deprecation-rules": ">=0.12",
        "phpstan/phpstan-phpunit": ">=0.12",
        "phpstan/phpstan-strict-rules": ">=0.12",
        "phpunit/phpunit": ">=9.3 <10",
        "psalm/plugin-phpunit": ">=0.10",
        "vimeo/psalm": ">=4"
    },
    "extra": {
        "unused": [
            "bentools/iterable-functions"
        ]
    },
    "autoload": {
        "psr-4": {
            "Heptacom\\HeptaConnect\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Heptacom\\HeptaConnect\\Core\\Test\\": "test/"
        }
    },
    "repositories": {
        "heptaconnect-sources": {
            "type": "path",
            "url": "../**"
        }
    },
    "support": {
        "issues": "https://github.com/HEPTACOM/heptaconnect-core/issues",
        "source": "https://github.com/HEPTACOM/heptaconnect-core",
        "docs": "https://connect.heptacom.de/",
        "rss": "https://connect.heptacom.de/feed/rss2.xml"
    },
    "website": "https://heptaconnect.io"
}
